Cell["\<\
the k integrand from the ground state\
\>", "Section",
 CellChangeTimes->{{3.6793165257661533`*^9, 3.6793165339906235`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"ggintegrandgd", "[", "k_", "]"}], ":=", 
  FractionBox[
   RowBox[{
    SuperscriptBox["A", "4"], " ", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{
      RowBox[{"-", "k"}], " ", "\[CurlyEpsilon]"}]], " ", "k", " ", 
    SuperscriptBox[
     RowBox[{"ftil", "[", "k", "]"}], "2"], " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", 
       FractionBox[
        RowBox[{"2", " ", 
         SuperscriptBox[
          RowBox[{"Sin", "[", 
           RowBox[{"c", " ", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"-", "k"}], "+", "\[CapitalOmega]"}], ")"}]}], "]"}], 
          "2"]}], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           RowBox[{"-", "k"}], "+", "\[CapitalOmega]"}], ")"}], "2"]]}], "-", 
      FractionBox[
       RowBox[{"2", " ", 
        SuperscriptBox[
         RowBox[{"Sin", "[", 
          RowBox[{"c", " ", 
           RowBox[{"(", 
            RowBox[{"k", "+", "\[CapitalOmega]"}], ")"}]}], "]"}], "2"]}], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"k", "+", "\[CapitalOmega]"}], ")"}], "2"]]}], ")"}]}], 
   RowBox[{"2", " ", "\[Pi]", " ", 
    SuperscriptBox["r", "2"]}]]}]], "Input",
 CellChangeTimes->{{3.679240022846974*^9, 3.679240033247569*^9}, {
  3.6793165387228937`*^9, 3.679316538891904*^9}, {3.6793174264576693`*^9, 
