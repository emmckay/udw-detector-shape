   RowBox[{"k_", ",", "\[Sigma]_"}], "]"}], ":=", 
  RowBox[{"FourierTransform", "[", 
   RowBox[{
    RowBox[{"f", "[", 
     RowBox[{"x", ",", "\[Sigma]"}], "]"}], ",", "x", ",", "k", ",", 
    RowBox[{"FourierParameters", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{"1", ",", 
       RowBox[{"-", "1"}]}], "}"}]}], ",", 
    RowBox[{"Assumptions", "\[Rule]", 
     RowBox[{
