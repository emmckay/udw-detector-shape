Cell[BoxData[{
 RowBox[{
  RowBox[{"f", "[", "x_", "]"}], ":=", 
  RowBox[{
   FractionBox["1", 
    RowBox[{"\[Sigma]", "*", 
     RowBox[{"\[Sqrt]", "\[Pi]"}]}]], 
