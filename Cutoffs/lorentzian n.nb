  RowBox[{"ftil", "[", "k_", "]"}], ":=", 
  RowBox[{"FourierTransform", "[", 
   RowBox[{
    RowBox[{"f", "[", "x", "]"}], ",", "x", ",", "k", ",", 
    RowBox[{"FourierParameters", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{"1", ",", 
       RowBox[{"-", "1"}]}], "}"}]}], ",", 
    RowBox[{"Assumptions", "\[Rule]", 
     RowBox[{
      RowBox[{"\[Sigma]", "\[Element]", "Reals"}], "&&", 
      RowBox[{"\[Sigma]", ">", "0"}]}]}]}], "]"}], " "}]}], "Input",
 CellChangeTimes->{{3.6781275509306498`*^9, 3.6781275535106544`*^9}, {
